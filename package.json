{
  "name": "@responsekit/nestjs",
  "version": "1.0.1",
  "description": "Helpers for integrating responsekit into nestjs.",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "scripts": {
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest",
    "tsc": "tsc -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IRCraziestTaxi/responsekit-nestjs.git"
  },
  "keywords": [
    "nestjs",
    "responsekit"
  ],
  "author": "IRCraziestTaxi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IRCraziestTaxi/responsekit-nestjs/issues"
  },
  "homepage": "https://github.com/IRCraziestTaxi/responsekit-nestjs#readme",
  "devDependencies": {
    "@nestjs/common": "^10.0.4",
    "@nestjs/cqrs": "^10.0.1",
    "@nestjs/testing": "^10.0.4",
    "@responsekit/core": "^1.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "eslint": "^8.44.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "@responsekit/core": "^1.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "tsconfig": "./tsconfig.test.json"
      }
    }
  },
  "files": [
    "LICENSE",
    "package.json",
    "dist/**"
  ]
}
